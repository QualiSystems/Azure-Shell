        <Rule Name="Configuration" />
        <Rule Name="Setting" />
      </Rules>
    </AttributeInfo>
    <AttributeInfo DefaultValue="true" Description="Enables the automatic execution of the Autoload command during reservation Setup." IsReadOnly="false" Name="Autoload" Type="Boolean">
