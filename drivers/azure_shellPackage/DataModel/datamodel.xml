      <Rules>
        <Rule Name="Configuration" />
        <Rule Name="Setting" />
      </Rules>
    </AttributeInfo>
    <AttributeInfo Name="Image OS Type"  DefaultValue="" Description="Operating system type to be installed on the VM. (Linux or Windows)" IsReadOnly="false" Type="String">
